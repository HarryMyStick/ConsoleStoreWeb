{# templates/base.html.twig #}
<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8">
		<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
		<title>
			{% block title %}HTT Console Shop
			{% endblock %}
		</title>
		{% block stylesheets %}
			<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css">
			<link id="u-theme-google-font" rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:100,100i,300,300i,400,400i,500,500i,700,700i,900,900i|Open+Sans:300,300i,400,400i,500,500i,600,600i,700,700i,800,800i">
		{% endblock %}
			<link href="{{ asset('css/home.css') }}" rel="stylesheet">
			<link href="{{ asset('css/sb-admin-2.min.css') }}" rel="stylesheet">
			<link rel="stylesheet" href="{{ asset('css/nicepage.css') }}" media="screen">
		{% block javascripts %}
			<script class="u-script" type="text/javascript" src="{{ asset('js/jquery.js') }}"></script>
			<script class="u-script" type="text/javascript" src="{{ asset('js/nicepage.js') }}"></script>
		{% endblock %}


	</head>
	<body>
		{% block header %}
			<header class="u-clearfix u-header u-header" id="sec-2f4c">
				<div class="u-clearfix u-sheet u-sheet-1">
					<nav class="u-menu u-menu-dropdown u-offcanvas u-menu-1">
						<div class="menu-collapse" style="font-size: 1rem; letter-spacing: 0px;">
							<a class="u-button-style u-custom-left-right-menu-spacing u-custom-padding-bottom u-custom-top-bottom-menu-spacing u-nav-link u-text-active-palette-1-base u-text-hover-palette-2-base" href="#">
								<svg class="u-svg-link" viewbox="0 0 24 24">
									<use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#menu-hamburger"></use>
								</svg>
								<svg class="u-svg-content" version="1.1" id="menu-hamburger" viewbox="0 0 16 16" x="0px" y="0px" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns="http://www.w3.org/2000/svg">
									<g>
										<rect y="1" width="16" height="2"></rect>
										<rect y="7" width="16" height="2"></rect>
										<rect y="13" width="16" height="2"></rect>
									</g>
								</svg>
							</a>
						</div>
						<div class="u-nav-container">
							<ul class="u-nav u-unstyled u-nav-1">
								<li class="u-nav-item navbar-brand">
									<a class=" u-button-style u-nav-link u-text-active-palette-1-base u-text-hover-palette-2-base" href="{{ path('home') }}" style="padding: 10px 20px; ">HTT Console Shop</a>
								</li>
								<li class="u-nav-item">
									<a class="u-button-style u-nav-link u-text-active-palette-1-base u-text-hover-palette-2-base" href="{{ path('app_about') }}" style="padding: 10px 10px;">About</a>
								</li>
								<li class="u-nav-item">
									<a class="u-button-style u-nav-link u-text-active-palette-1-base u-text-hover-palette-2-base" href="{{ path('app_login') }}" style="padding: 10px 10px;">Login</a>
								</li>
							</ul>
						</div>
					</nav>
					<!--shopping_cart-->
					<a class="u-shopping-cart u-shopping-cart-1" href="{{ path('cart') }}">
						<span class="u-icon u-shopping-cart-icon u-icon-1">
							<svg class="u-svg-link" preserveaspectratio="xMidYMin slice" viewbox="0 0 16 16" style="">
								<use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#svg-9202"></use>
							</svg>
							<svg class="u-svg-content" viewbox="0 0 16 16" x="0px" y="0px" id="svg-9202">
								<path d="M14.5,3l-2.1,5H6.1L5.9,7.6L4,3H14.5 M0,0v1h2.1L5,8l-2,4h11v-1H4.6l1-2H13l3-7H3.6L2.8,0H0z M12.5,13
																												c-0.8,0-1.5,0.7-1.5,1.5s0.7,1.5,1.5,1.5s1.5-0.7,1.5-1.5S13.3,13,12.5,13L12.5,13z M4.5,13C3.7,13,3,13.7,3,14.5S3.7,16,4.5,16
																												S6,15.3,6,14.5S5.3,13,4.5,13L4.5,13z"></path>
							</svg>
							<span class="u-icon-circle u-palette-1-base u-shopping-cart-count" style="font-size: 0.75rem;"></span>
						</span>
					</a>
					<!--/shopping_cart-->
				</div>
			</header>
		{% endblock %}
		{% block body %}{% endblock %}
		{% block footer %}
			<!-- Footer -->
			<footer class="sticky-footer bg-dark">
				<div class="container my-auto">
					<div class="copyright text-center my-auto text-light">
						<span>Copyright &copy; HTT Website 2022</span>
					</div>
				</div>
			</footer>
			<!-- End of Footer -->
		{% endblock %}
	</body>
</html></body></body></html></body></html>
